<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="triangle" class = "prepare.interview.Triangle">
		<property name="points">
			<list>
				<ref bean="point0"/>
				<ref bean="point1"/>
				<ref bean="point2"/>
			</list>
		</property>
	</bean>
	<bean id="point1" class="prepare.interview.Point">
		<property name="x" value="20"/>
		<property name="y" value="0"/>
	</bean>
	<bean id="point2" class="prepare.interview.Point">
		<property name="x" value="-20"/>
		<property name="y" value="0"/>
	</bean>
	<bean id="point0" class="prepare.interview.Point">
		<property name="x" value="0"/>
		<property name="y" value="0"/>
	</bean>
	<alias name="triangle" alias="triangle-alias"/>
</beans>